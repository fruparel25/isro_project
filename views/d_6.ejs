<script>
$(document).ready(function(){
  $.getJSON("/december_21_calc.json",function(data){
    var temp_data='';
    $.each(data,function(key,value){
      temp_data+='<tr>';
      temp_data+='<td>'+value.OBJECTID+'</td>';
      temp_data+='<td>'+value.SHAPE_Leng+'</td>';
      temp_data+='<td>'+value.SHAPE_Area+'</td>';
      temp_data+='<td>'+value.MEAN+'</td>';
      temp_data+='<td>'+value.sunlight_percent_6+'</td>';
      temp_data+='<td>'+value.energy_6+'</td>';
      temp_data+='<td>'+value.shadow_free_area_6+'</td>';
      temp_data+='</tr>';
    });
    $('#temp_table').append(temp_data);
  });
});
</script>
    <script>
