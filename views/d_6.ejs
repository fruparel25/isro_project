<div class="container">
    <div class="table-responsive">
      <h1 style="background-color:rgb(213, 217, 218); text-align:center"><u>Energy And Shadow_free_area </u></h1>
      <table class="table table-bordered table-striped" id="temp_table">
        <tr>
          <th>ObjectId</th>
          <th>Shape Length</th>
          <th>Shape Area</th>
          <th>MEAN</th>
          <th>Sunlight Percent(%)</th>
          <th>Energy(KW)</th>
          <th>Shadow Free Area</th>
        </tr>
      </table>
    </div>
  </div>
